    <com.lconsulting.sudoku.ui.view.SquareView
        android:id="@+id/gsquare9"
        android:layout_width="@dimen/touchpad_square_size"
        android:layout_height="@dimen/touchpad_square_size"
        android:tag="9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/gsquare8"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/gsquare6" />

</merge>