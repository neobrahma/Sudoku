    <com.lconsulting.sudoku.ui.view.SquareView
        android:id="@+id/gsquare7"
        android:layout_width="@dimen/touchpad_square_size"
        android:layout_height="@dimen/touchpad_square_size"
        android:tag="7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/gsquare8"
        app:layout_constraintTop_toBottomOf="@id/gsquare4" />

    <com.lconsulting.sudoku.ui.view.SquareView
